   "source": [
    "# divisibility test\n",
    "a= int(input())\n",
    "b= int(input())\n",
    "if a%b==0:\n",
    "    print (f\"{a} is divisible by {b}\")\n",
    "else :\n",
    "    print (f\"{a} is not divisible by {b}\")\n",
    "i=0\n",
    "x=a\n",
    "while x%b==0:\n",
    "    i=i+1\n",
    "    x=x//b\n",
    "    if x==b:\n",
    "        break\n",
    " \n",
    "print(f\"{a} is divisible {i} times by {b}\")     \n",
    "    \n",
    " "
